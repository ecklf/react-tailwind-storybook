{
  "name": "react-tailwind-storybook",
  "version": "1.0.0",
  "author": "impulse <ecklf@icloud.com>",
  "license": "MIT",
  "types": "build/index.d.ts",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "typings": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "start:storybook": "start-storybook",
    "build:tailwind": "postcss src/css/index.src.css -o src/css/index.css",
    "watch:tailwind": "postcss -w src/css/index.src.css -o src/css/index.css",
    "watch:rollup": "rollup -c rollup.config.js --watch",
    "storybook": "run-p watch:tailwind start:storybook",
    "dev": "run-p watch:tailwind watch:rollup",
    "build": "export NODE_ENV=production && rollup -c rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/theming": "^5.3.18",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/storybook__react": "^5.2.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "cssnano": "^4.1.10",
    "jest": "^25.5.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "rollup": "^2.7.6",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "tailwindcss": "^1.4.4",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
